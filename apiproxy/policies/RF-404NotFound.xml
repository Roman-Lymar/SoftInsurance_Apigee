<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<RaiseFault name="RF-404NotFound">
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <FaultResponse>
        <Set>
            <StatusCode>404</StatusCode>
            <ReasonPhrase>Not Found</ReasonPhrase>
        </Set>
    </FaultResponse>
</RaiseFault>